{
	"compilerOptions": {
		"target": "ES2021",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": false,
		"noImplicitReturns": false,
		"strictPropertyInitialization": false,
		"lib": [
          "es2016",
          "dom"
        ]
    },
	"types": [ "node" ],
	"typeRoots": [ "./node_modules/@types" ],
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Recommended"
}
