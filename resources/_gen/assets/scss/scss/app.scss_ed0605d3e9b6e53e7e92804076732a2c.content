  text-decoration: none;
  transition: color 200ms ease;
  will-change: color; }
  .btn:hover {
    color: #FF5043; }
  .btn-loading {
    transition: none;
    pointer-events: none;
    color: transparent !important; }
    .btn-loading:after {
      content: "";
      width: 18px;
      height: 18px;
      border: 2px solid rgba(0, 0, 0, 0.5);
      border-right-color: transparent !important;
      border-radius: 50%;
      display: inline-block;
