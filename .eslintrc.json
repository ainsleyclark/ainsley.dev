{
	"root": true,
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": [
			"./tsconfig.json"
		]
	},
	"rules": {
		"@typescript-eslint/ban-ts-ignore": "off"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"ignorePatterns": [
		"public/**/.js",
		"staic/**"
	]
}
