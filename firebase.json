{
	"hosting": {
		"public": "public",
		"cleanUrls": true,
		"trailingSlash": true,
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"headers": [
			{
				"source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
				"headers": [
					{
						"key": "Access-Control-Allow-Origin",
						"value": "*"
					}
				]
			}
		]
	}
}
